#include <stdio.h>

int main() {
	int Test_case,A,B,C,sum=0;
	scanf("%d",&Test_case);
	for(int i=0;i<Test_case;i++){
	    scanf("%d",&A);
	    scanf("%d",&B);
	    scanf("%d",&C);
	    if(((A+B)%2!=0) || ((C+B)%2!=0) || ((A+C)%2!=0) ){
	        printf("YES\n");
	    }
	    else{
	        printf("NO\n");
	    }
	}
	return 0;

}

